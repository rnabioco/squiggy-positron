{
  "name": "squiggy-positron",
  "displayName": "Squiggy",
  "description": "Visualize Oxford Nanopore sequencing data (squiggle plots) from POD5 files",
  "version": "0.1.23-alpha",
  "publisher": "rnabioco",
  "icon": "resources/icon-128.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/rnabioco/squiggy-positron.git"
  },
  "engines": {
    "vscode": "^1.96.0",
    "positron": "^2025.6.0"
  },
  "categories": [
    "Data Science",
    "Visualization"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./build/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "squiggy.openPOD5",
        "title": "Open POD5 File",
        "category": "Squiggy",
        "icon": "$(file-binary)",
        "enablement": "squiggy.packageInstalled"
      },
      {
        "command": "squiggy.openBAM",
        "title": "Open BAM File",
        "category": "Squiggy",
        "icon": "$(file-code)",
        "enablement": "squiggy.packageInstalled"
      },
      {
        "command": "squiggy.closePOD5",
        "title": "Close POD5 File",
        "category": "Squiggy",
        "icon": "$(close)",
        "enablement": "squiggy.packageInstalled"
      },
      {
        "command": "squiggy.closeBAM",
        "title": "Close BAM File",
        "category": "Squiggy",
        "icon": "$(close)",
        "enablement": "squiggy.packageInstalled"
      },
      {
        "command": "squiggy.loadTestData",
        "title": "Load Test Data",
        "category": "Squiggy",
        "icon": "$(database)",
        "enablement": "squiggy.packageInstalled"
      },
      {
        "command": "squiggy.plotRead",
        "title": "Plot Selected Read(s)",
        "category": "Squiggy",
        "icon": "$(graph)",
        "enablement": "squiggy.packageInstalled"
      },
      {
        "command": "squiggy.plotAggregate",
        "title": "Plot Aggregate for Reference",
        "category": "Squiggy",
        "icon": "$(graph-line)",
        "enablement": "squiggy.packageInstalled"
      },
      {
        "command": "squiggy.refreshReads",
        "title": "Refresh Read List",
        "category": "Squiggy",
        "icon": "$(refresh)",
        "enablement": "squiggy.packageInstalled"
      },
      {
        "command": "squiggy.clearState",
        "title": "Clear All State (After Kernel Restart)",
        "category": "Squiggy",
        "icon": "$(clear-all)"
      },
      {
        "command": "squiggy.restartBackgroundKernel",
        "title": "Restart Squiggy Background Kernel",
        "category": "Squiggy",
        "icon": "$(debug-restart)"
      },
      {
        "command": "squiggy.debugModificationsPanel",
        "title": "Debug Modifications Panel",
        "category": "Squiggy",
        "icon": "$(bug)"
      },
      {
        "command": "squiggy.openFASTA",
        "title": "Open FASTA File",
        "category": "Squiggy",
        "icon": "$(file-text)",
        "enablement": "squiggy.packageInstalled"
      },
      {
        "command": "squiggy.closeFASTA",
        "title": "Close FASTA File",
        "category": "Squiggy",
        "icon": "$(close)",
        "enablement": "squiggy.packageInstalled"
      },
      {
        "command": "squiggy.plotMotifAggregate",
        "title": "Plot Motif Aggregate",
        "category": "Squiggy",
        "icon": "$(graph-scatter)",
        "enablement": "squiggy.packageInstalled"
      },
      {
        "command": "squiggy.loadSample",
        "title": "Load Sample (Multi-Sample Comparison)",
        "category": "Squiggy",
        "icon": "$(folder-opened)",
        "enablement": "squiggy.packageInstalled"
      },
      {
        "command": "squiggy.loadTestMultiReadDataset",
        "title": "Load Test Multi-Read Dataset",
        "category": "Squiggy",
        "icon": "$(database)",
        "enablement": "squiggy.packageInstalled"
      },
      {
        "command": "squiggy.plotDeltaComparison",
        "title": "Plot Delta Comparison",
        "category": "Squiggy",
        "icon": "$(compare)",
        "enablement": "squiggy.packageInstalled"
      },
      {
        "command": "squiggy.loadDemoSession",
        "title": "Load Demo Session",
        "category": "Squiggy",
        "icon": "$(rocket)",
        "enablement": "squiggy.packageInstalled"
      },
      {
        "command": "squiggy.saveSession",
        "title": "Save Session",
        "category": "Squiggy",
        "icon": "$(save)",
        "enablement": "squiggy.packageInstalled"
      },
      {
        "command": "squiggy.restoreSession",
        "title": "Restore Session",
        "category": "Squiggy",
        "icon": "$(history)",
        "enablement": "squiggy.packageInstalled"
      },
      {
        "command": "squiggy.exportSession",
        "title": "Export Session to File",
        "category": "Squiggy",
        "icon": "$(export)",
        "enablement": "squiggy.packageInstalled"
      },
      {
        "command": "squiggy.importSession",
        "title": "Import Session from File",
        "category": "Squiggy",
        "icon": "$(import)",
        "enablement": "squiggy.packageInstalled"
      },
      {
        "command": "squiggy.clearSession",
        "title": "Clear Saved Session",
        "category": "Squiggy",
        "icon": "$(trash)",
        "enablement": "squiggy.packageInstalled"
      },
      {
        "command": "squiggy.loadPipelineSession",
        "title": "Load Pipeline Session",
        "category": "Squiggy",
        "icon": "$(folder-library)",
        "enablement": "squiggy.packageInstalled"
      },
      {
        "command": "squiggy.showLogs",
        "title": "Show Extension Logs",
        "category": "Squiggy",
        "icon": "$(output)"
      },
      {
        "command": "squiggy.resetVenv",
        "title": "Reset Python Environment",
        "category": "Squiggy",
        "icon": "$(refresh)"
      },
      {
        "command": "squiggy.setLogLevel",
        "title": "Set Log Level",
        "category": "Squiggy",
        "icon": "$(settings-gear)"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "squiggy-sidebar",
          "title": "Squiggy",
          "icon": "resources/icon-sidebar.svg"
        }
      ]
    },
    "views": {
      "squiggy-sidebar": [
        {
          "id": "squiggyComparisonSamples",
          "name": "Samples",
          "type": "webview"
        },
        {
          "id": "squiggySessionPanel",
          "name": "Session Manager",
          "type": "webview"
        },
        {
          "id": "squiggyReadList",
          "name": "Read Explorer",
          "type": "webview",
          "visibility": "collapsed"
        },
        {
          "id": "squiggyPlotOptions",
          "name": "Plotting",
          "type": "webview"
        },
        {
          "id": "squiggyModificationsPanel",
          "name": "Modifications Explorer",
          "type": "webview"
        },
        {
          "id": "squiggyMotifSearch",
          "name": "Motif Explorer",
          "type": "webview",
          "visibility": "collapsed"
        }
      ]
    },
    "menus": {
      "view/title": [
        {
          "command": "squiggy.refreshReads",
          "when": "view == squiggyReadList",
          "group": "navigation"
        }
      ],
      "view/item/context": [
        {
          "command": "squiggy.plotRead",
          "when": "view == squiggyReadList && viewItem == read",
          "group": "inline"
        },
        {
          "command": "squiggy.plotAggregate",
          "when": "view == squiggyReadList && viewItem == reference",
          "group": "inline"
        }
      ]
    },
    "configuration": {
      "title": "Squiggy",
      "properties": {
        "squiggy.defaultPlotMode": {
          "type": "string",
          "enum": [
            "SINGLE",
            "EVENTALIGN"
          ],
          "default": "SINGLE",
          "description": "Default plot mode for visualizations"
        },
        "squiggy.defaultNormalization": {
          "type": "string",
          "enum": [
            "NONE",
            "ZNORM",
            "MEDIAN",
            "MAD"
          ],
          "default": "ZNORM",
          "description": "Default signal normalization method"
        },
        "squiggy.downsampleThreshold": {
          "type": "number",
          "default": 100000,
          "description": "Downsample signals with more than this many samples"
        },
        "squiggy.theme": {
          "type": "string",
          "enum": [
            "LIGHT",
            "DARK"
          ],
          "default": "LIGHT",
          "description": "Plot color theme"
        },
        "squiggy.aggregateSampleSize": {
          "type": "number",
          "default": 100,
          "minimum": 10,
          "maximum": 10000,
          "description": "Maximum number of reads to sample for aggregate plots"
        },
        "squiggy.logLevel": {
          "type": "string",
          "enum": [
            "DEBUG",
            "INFO",
            "WARNING",
            "ERROR"
          ],
          "default": "INFO",
          "description": "Minimum log level to display in Output channel (DEBUG = most verbose, ERROR = least verbose)"
        }
      }
    }
  },
  "scripts": {
    "sync": "node scripts/sync-version.js",
    "vscode:prepublish": "npm run sync && npm run compile",
    "compile": "npm run sync && webpack --mode production",
    "watch": "webpack --mode development --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src --ext ts,tsx",
    "lint:fix": "eslint src --ext ts,tsx --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx}\"",
    "package": "vsce package --out build/"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.0",
    "@jest/globals": "^30.2.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^30.0.0",
    "@types/node": "22.x",
    "@types/react": "18.3.1",
    "@types/react-dom": "18.3.1",
    "@types/react-window": "^1.8.5",
    "@types/vscode": "^1.96.0",
    "@typescript-eslint/eslint-plugin": "^8.46.2",
    "@typescript-eslint/parser": "^8.46.2",
    "@vscode/vsce": "^3.6.2",
    "copy-webpack-plugin": "^13.0.1",
    "css-loader": "^6.9.1",
    "eslint": "^9.39.0",
    "globals": "^16.5.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "ovsx": "^0.10.6",
    "prettier": "^3.6.2",
    "style-loader": "^3.3.2",
    "ts-jest": "^29.4.5",
    "ts-loader": "^9.5.1",
    "typescript": "^5.7.2",
    "webpack": "^5.94.0",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "@posit-dev/positron": "^0.1.0",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-window": "^1.8.10"
  }
}
